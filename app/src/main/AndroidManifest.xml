<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ingchristopher.petagramfinal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dog_footprint_48"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityFavs" />
        <activity android:name=".DatosDesarrollador" />
        <activity android:name=".DatosContacto" />
        <activity android:name=".vista.fragments.PerfilFragment" />
        <activity android:name=".vista.fragments.RecyclerViewFragment"></activity>
    </application>

</manifest>